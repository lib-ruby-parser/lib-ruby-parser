--INPUT
/\xa8/n =~ ""
--LOCATIONS
~~~~~~~~~~~~~ expression (root)
~~~~~~~~~~~~~ expression (root)
~ begin (root -> re)
     ~ end (root -> re)
~~~~~~~ expression (root -> re)
 ~~~~ expression (root -> re -> parts -> 0)
      ~ expression (root -> re -> options)
           ~ begin (root -> value)
            ~ end (root -> value)
           ~~ expression (root -> value)
--AST
s(:match_with_lvasgn,
  s(:regexp,
    s(:str, "\\xa8"),
    s(:regopt, "n")),
  s(:str, ""))
