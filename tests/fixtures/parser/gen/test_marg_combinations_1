--INPUT
def f ((a, a1)); end
--LOCATIONS
~~~ keyword ()
    ~ name ()
                 ~~~ end ()
~~~~~~~~~~~~~~~~~~~~ expression ()
      ~ begin (args)
              ~ end (args)
      ~~~~~~~~~ expression (args)
       ~ begin (args/arg[0])
             ~ end (args/arg[0])
       ~~~~~~~ expression (args/arg[0])
        ~ name (args/arg[0]/item[0])
        ~ expression (args/arg[0]/item[0])
           ~~ name (args/arg[0]/item[1])
           ~~ expression (args/arg[0]/item[1])
--AST
s(:def, "f",
  s(:args,
    s(:mlhs,
      s(:arg, "a"),
      s(:arg, "a1"))), nil)
