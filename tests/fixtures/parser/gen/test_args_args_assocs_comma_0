--INPUT
foo[bar, :baz => 1,]
--LOCATIONS
   ~ begin ()
                   ~ end ()
~~~~~~~~~~~~~~~~~~~~ expression ()
~~~ expression (recv)
    ~~~ expression (index[0])
         ~~~~~~~~~ expression (index[1])
              ~~ operator (index[1]/pair[0])
         ~~~~~~~~~ expression (index[1]/pair[0])
         ~ begin (index[1]/pair[0]/key)
         ~~~~ expression (index[1]/pair[0]/key)
                 ~ expression (index[1]/pair[0]/value)
--AST
s(:index,
  s(:lvar, "foo"),
  s(:lvar, "bar"),
  s(:kwargs,
    s(:pair,
      s(:sym, "baz"),
      s(:int, "1"))))
