--INPUT
1 => [a]; a
--LOCATIONS
~~~~~~~~~~~ expression ()
  ~~ operator (stmt[0])
~~~~~~~~ expression (stmt[0])
~ expression (stmt[0]/value)
     ~ begin (stmt[0]/pattern)
       ~ end (stmt[0]/pattern)
     ~~~ expression (stmt[0]/pattern)
      ~ name (stmt[0]/pattern/element[0])
      ~ expression (stmt[0]/pattern/element[0])
          ~ expression (stmt[1])
--AST
s(:begin,
  s(:in_match,
    s(:int, "1"),
    s(:array_pattern,
      s(:match_var, "a"))),
  s(:lvar, "a"))
