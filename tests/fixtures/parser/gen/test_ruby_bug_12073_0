--INPUT
a = 1; a b: 1
--LOCATIONS
~~~~~~~~~~~~~ expression ()
  ~ operator (stmt[0])
~ name (stmt[0])
~~~~~ expression (stmt[0])
    ~ expression (stmt[0]/value)
       ~ selector (stmt[1])
       ~~~~~~ expression (stmt[1])
         ~~~~ expression (stmt[1]/arg[0])
          ~ operator (stmt[1]/arg[0]/pair[0])
         ~~~~ expression (stmt[1]/arg[0]/pair[0])
         ~ expression (stmt[1]/arg[0]/pair[0]/key)
            ~ expression (stmt[1]/arg[0]/pair[0]/value)
--AST
s(:begin,
  s(:lvasgn, "a",
    s(:int, "1")),
  s(:send, nil, "a",
    s(:kwargs,
      s(:pair,
        s(:sym, "b"),
        s(:int, "1")))))
